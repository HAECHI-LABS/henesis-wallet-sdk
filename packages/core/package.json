{
  "name": "@haechi-labs/henesis-wallet-core",
  "version": "1.0.7",
  "description": "henesis wallet sdk core module",
  "author": "Haechi-labs <haechi@haechi.io>",
  "homepage": "",
  "license": "ISC",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "resources"
  ],
  "scripts": {
    "lint:fix": "eslint 'src/**/*.ts' '__tests__/**/*.ts' --fix",
    "codegen:btc": "java -jar ./scripts/swagger-codegen-cli-2.4.10.jar generate -i http://localhost:8080/v2/api-docs -l typescript-fetch -o ./src/__generate__/btc --type-mappings Date=string",
    "codegen:eth": "java -jar ./scripts/swagger-codegen-cli-2.4.10.jar generate -i https://dev.wallet.henesis.io/v2/api-docs -l typescript-fetch -o ./src/__generate__/eth --type-mappings Date=string",
    "test": "jest",
    "build": "rm -rf ./lib && tsc",
    "eslint": "./node_modules/.bin/eslint --ext .js,.jsx,.ts,.tsx ./src",
    "eslint:fix": "./node_modules/.bin/eslint --ext .js,.jsx,.ts,.tsx ./src --fix",
    "prettier:example": "prettier './example/**/*.{js,ts,tsx}' --write",
    "prettier": "prettier './src/**/*.{js,ts,tsx}' --write",
    "preBootstrap": "rm -rf ./node_modules"
  },
  "dependencies": {
    "@types/aes-js": "^3.1.1",
    "@types/bignumber.js": "^5.0.0",
    "@types/bip38": "^2.0.1",
    "@types/bitcoinjs-lib": "^5.0.0",
    "@types/crypto-js": "^3.1.45",
    "@types/jest": "^24.0.15",
    "@types/wif": "^2.0.1",
    "aes-js": "^3.1.2",
    "axios": "^0.19.2",
    "bip38": "^3.1.0",
    "bitcoinjs-lib": "^5.1.8",
    "bn.js": "^5.1.1",
    "crypto-js": "^3.1.1",
    "dotenv": "^8.2.0",
    "jest": "^24.8.0",
    "jest-extended": "^0.11.1",
    "js-base64": "^2.5.2",
    "path": "^0.12.7",
    "pdfkit": "^0.11.0",
    "portable-fetch": "^3.0.0",
    "qrcode": "^1.4.4",
    "svg-to-pdfkit": "^0.1.8",
    "ts-jest": "^24.0.2",
    "typescript": "^3.8.2",
    "web3": "^1.2.6",
    "web3-eth": "^1.2.6"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.jest.json",
        "diagnostics": true
      }
    },
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@types/nock": "^11.1.0",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.3",
    "nock": "^12.0.3",
    "prettier": "^2.0.5"
  },
  "gitHead": "ca83cd8dcac533beb9e12d76ed1047fb29d8aea7"
}
