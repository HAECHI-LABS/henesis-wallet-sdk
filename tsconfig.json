{
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "outDir": "./dist",
    "strict": false,
    "noImplicitAny": false,
    "baseUrl": "./src/",
    "paths": {
      "#/*": [
        "*"
      ]
    }
  },
  "include": [
    "packages/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
