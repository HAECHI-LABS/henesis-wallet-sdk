{
  "name": "@haechi-labs/henesis-wallet-core",
  "version": "3.3.4-alpha.2",
  "description": "henesis wallet sdk core module",
  "author": "Haechi-labs <haechi@haechi.io>",
  "homepage": "",
  "license": "ISC",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "resources"
  ],
  "scripts": {
    "node:example": "ts-node --files ./example/change-account-role.ts",
    "codegen": "concurrently \"npm run codegen:btc\" \"npm run codegen:eth\" \"npm run codegen:fil\" \"npm run codegen:ltc\" \"npm run codegen:bch\" \"npm run codegen:accounts\"",
    "codegen:btc": "java -jar ./scripts/openapi-generator-cli-4.3.1.jar generate -i https://dev.wallet.henesis.io/docs/v2/btc/v3/api-docs -g typescript-axios -o ./src/__generate__/btc --type-mappings Date=string --skip-validate-spec --config ./codegenConfig.json",
    "codegen:eth": "java -jar ./scripts/openapi-generator-cli-4.3.1.jar generate -i https://dev.wallet.henesis.io/docs/v2/eth/v3/api-docs -g typescript-axios -o ./src/__generate__/eth --type-mappings Date=string --skip-validate-spec --config ./codegenConfig.json",
    "codegen:fil": "java -jar ./scripts/openapi-generator-cli-4.3.1.jar generate -i https://dev.wallet.henesis.io/docs/v2/fil/v3/api-docs -g typescript-axios -o ./src/__generate__/fil --type-mappings Date=string --skip-validate-spec --config ./codegenConfig.json",
    "codegen:ltc": "java -jar ./scripts/openapi-generator-cli-4.3.1.jar generate -i https://dev.wallet.henesis.io/docs/v2/ltc/v3/api-docs -g typescript-axios -o ./src/__generate__/ltc --type-mappings Date=string --skip-validate-spec --config ./codegenConfig.json",
    "codegen:bch": "java -jar ./scripts/openapi-generator-cli-4.3.1.jar generate -i https://dev.wallet.henesis.io/docs/v2/bch/v3/api-docs -g typescript-axios -o ./src/__generate__/bch --type-mappings Date=string --skip-validate-spec --config ./codegenConfig.json",
    "codegen:accounts": "java -jar ./scripts/openapi-generator-cli-4.3.1.jar generate -i https://dev.wallet.henesis.io/docs/v2/accounts/v3/api-docs -g typescript-axios -o ./src/__generate__/accounts --type-mappings Date=string --skip-validate-spec --config ./codegenConfig.json",
    "test": "jest",
    "test:integration": "jest --config ./jest-integration.json",
    "type-check": "tsc --noEmit",
    "build": "rm -rf ./lib && tsc",
    "build:windows": "npm run preBuild:windows && mkdir lib && tsc",
    "eslint": "../../node_modules/.bin/eslint --ext .js,.jsx,.ts,.tsx ./src",
    "eslint:fix": "../../node_modules/.bin/eslint --ext .js,.jsx,.ts,.tsx ./src --fix",
    "prettier:example": "prettier './example/**/*.{js,ts,tsx}' --write",
    "prettier": "prettier './src/**/*.{js,ts,tsx}' --write",
    "preBuild:windows": "if exist lib rmdir /q /s lib",
    "preBootstrap": "rm -rf ./node_modules",
    "preBootstrap:windows": "if exist node_modules rmdir /q /s node_modules"
  },
  "dependencies": {
    "@types/aes-js": "^3.1.1",
    "@types/bignumber.js": "^5.0.0",
    "@types/bip38": "^2.0.1",
    "@types/bitcoinjs-lib": "^5.0.0",
    "@types/crypto-js": "^3.1.45",
    "@types/lodash": "^4.14.155",
    "@types/node": "^14.0.19",
    "@types/wif": "^2.0.1",
    "aes-js": "^3.1.2",
    "async-retry": "^1.3.1",
    "axios": "^0.21.1",
    "base32-decode": "^1.0.0",
    "base32-encode": "^1.1.1",
    "bip32": "^2.0.6",
    "bip38": "^3.1.0",
    "bip39": "^3.0.4",
    "bitcoinjs-lib": "^5.1.8",
    "blakejs": "^1.1.0",
    "bn.js": "^5.1.1",
    "cashaddrjs": "^0.3.3",
    "chalk": "^4.1.0",
    "cids": "^0.7.5",
    "concurrently": "^5.2.0",
    "crypto-js": "^3.1.1",
    "dotenv": "^8.2.0",
    "eth-crypto": "^1.8.0",
    "faker": "^4.1.0",
    "ipld-dag-cbor": "^1.0.0",
    "jest": "^24.8.0",
    "jest-extended": "^0.11.1",
    "js-base64": "^2.5.2",
    "leb128": "0.0.5",
    "lodash": "^4.17.19",
    "lowercase-keys": "^2.0.0",
    "multihashing-async": "^2.1.2",
    "path": "^0.12.7",
    "pdfkit": "^0.11.0",
    "qrcode": "^1.4.4",
    "secp256k1": "^4.0.2",
    "svg-to-pdfkit": "^0.1.8",
    "ts-jest": "^24.0.2",
    "ts-retry": "^1.1.2",
    "typescript": "^3.8.3",
    "web3": "^1.3.6",
    "web3-eth": "^1.3.6",
    "web3-eth-contract": "^1.3.6",
    "web3-utils": "^1.3.6"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@types/nock": "^11.1.0",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.3",
    "nock": "^12.0.3",
    "prettier": "2.3.1",
    "ts-node": "^8.10.2"
  },
  "gitHead": "ca83cd8dcac533beb9e12d76ed1047fb29d8aea7"
}
